{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "uwu-radio",
  "title": "uwu radio",
  "description": "",
  "icon": "command-icon.png",
  "author": "lexisother",
  "categories": [
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "nowPlayingMenuBar",
      "title": "Menu Bar Player",
      "description": "See what's currently playing in your Menu Bar.",
      "mode": "menu-bar",
      "interval": "10s"
    }
  ],
  "dependencies": {
    "@microsoft/signalr": "^8.0.0",
    "@raycast/api": "^1.66.2",
    "@raycast/utils": "^1.4.8",
    "@vue-reactivity/watch": "^0.2.0",
    "@vue/reactivity": "^3.4.15",
    "cross-fetch": "^4.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
